<h1>Member Rental History</h1>
<% if @memberRentHist_array.length == 0 %>
    <h2> No rental history for this member! </h2> 
<% else %> 
    <table>
    <tr>
        <th>Make</th>
        <th>Model</th>
        <th>date</th>
    </tr>
    <% @memberRentHist_array.each do |memberRentHist| %>
        <tr>
            <td> <%= memberRentHist['make'] %></td>
            <td> <%= memberRentHist['model'] %></td>
            <td> <%= memberRentHist['date'] %></td>
        
        </tr>
    <% end %>
    </table>
<%end%>

